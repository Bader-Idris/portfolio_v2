{
  "name": "portfolio",
  "appId": "com.baderidris.www",
  "description": "Bader's portfolio using Vite + Vue 3 + Electron + capacitor + expressJs + Docker containerized environment",
  "homepage": "https://baderidris.com",
  "author": "Bader Idris <contact@baderidris.com>",
  "version": "1.0.1",
  "private": true,
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "dev:electron": "vite --config vite.config.electron.mts",
    "dev:debug": "vite -d",
    "dev:debug:force": "vite -d --force",
    "ionic:build": "bun run build",
    "ionic:serve": "bun run dev",
    "build": "bun run build:pre && electron-builder --config=buildAssets/builder/config.js",
    "build:electron": "vite build --config vite.config.electron.mts",
    "build:pre": "bun run format:fix && vue-tsc --noEmit && vite build && bun run build:electron",
    "build:all": "bun run build:pre && bun run build:electron && electron-builder --config=buildAssets/builder/config.js -wml",
    "build:dir": "bun run build:pre && bun run build:electron && electron-builder --config=buildAssets/builder/config.js --dir",
    "build:mac": "bun run build:pre && bun run build:electron && CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --config=buildAssets/builder/config.js --mac",
    "build:linux": "bun run build:pre && bun run build:electron && electron-builder --config=buildAssets/builder/config.js --linux",
    "build:win": "bun run build:pre && bun run build:electron && electron-builder --config=buildAssets/builder/config.js --windows",
    "preview": "vite preview # should be based on vite build",
    "lint": "eslint --ext .js,.ts,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.ts,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "format": "prettier .",
    "format:fix": "prettier . --write",
    "test": "bun run build:pre && playwright test",
    "test:linux": "bun run build:pre && xvfb-run --auto-servernum --server-args='-screen 0, 1280x960x24' -- playwright test",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:unit": "vitest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@capacitor-community/sqlite": "^6.0.2",
    "@capacitor/cli": "7.0.1",
    "@capacitor/core": "^7.0.1",
    "@ionic/vue": "^8.4.2",
    "@ionic/vue-router": "^8.4.2",
    "@vueuse/head": "^2.0.0",
    "animate.css": "^4.1.1",
    "canvas-confetti": "^1.9.3",
    "d3": "^7.9.0",
    "highlight.js": "^11.11.1",
    "howler": "^2.2.4",
    "ionicons": "^7.4.0",
    "lodash-es": "^4.17.21",
    "particles.js": "^2.0.0",
    "pinia": "^2.3.1",
    "vue": "^3.5.13",
    "vue-i18n": "^11.0.1",
    "vue-router": "^4.5.0",
    "vue3-toastify": "^0.2.8"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/eslint-parser": "^7.26.5",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-typescript": "^7.26.0",
    "@capacitor-community/app-icon": "^5.0.0",
    "@capacitor/action-sheet": "^7.0.0",
    "@capacitor/android": "^7.0.1",
    "@capacitor/app": "^7.0.0",
    "@capacitor/app-launcher": "^7.0.0",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/browser": "^7.0.0",
    "@capacitor/camera": "^7.0.0",
    "@capacitor/clipboard": "^7.0.0",
    "@capacitor/device": "^7.0.0",
    "@capacitor/geolocation": "^7.0.0",
    "@capacitor/haptics": "^7.0.0",
    "@capacitor/ios": "^7.0.1",
    "@capacitor/keyboard": "^7.0.0",
    "@capacitor/local-notifications": "^7.0.0",
    "@capacitor/network": "^7.0.0",
    "@capacitor/preferences": "^7.0.0",
    "@capacitor/push-notifications": "^7.0.0",
    "@capacitor/screen-orientation": "^7.0.0",
    "@capacitor/screen-reader": "^7.0.0",
    "@capacitor/share": "^7.0.0",
    "@capacitor/splash-screen": "^7.0.0",
    "@capacitor/status-bar": "^7.0.0",
    "@capacitor/text-zoom": "^7.0.0",
    "@capacitor/toast": "^7.0.0",
    "@playwright/test": "^1.50.0",
    "@rushstack/eslint-patch": "^1.10.5",
    "@tsconfig/node23": "^23.0.0",
    "@types/node": "^22.10.10",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitejs/plugin-legacy": "^6.0.0",
